<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="confirmEndDialog" title="Confirmación" type="Message">
        <content>
            <Text text="¿Está seguro de que desea terminar la jornada laboral?" />
        </content>
        <beginButton>
            <Button text="Sí" press="onConfirmEnd" />
        </beginButton>
        <endButton>
            <Button text="No" press="onCancelEnd" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
