{
    "_version": "1.1.0",

	"sap.app": {
		"id": "com.resulto.control",
		"type": "application",
		"applicationVersion": {
			"version": "${version}"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.resulto.control.view.Main",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.129.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.uxap": {},
				"sap.ui.comp": {},
				"sap.viz": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"config": {
			"fullWidth": true
		},
        "models": {
          "": {
            "type": "sap.ui.model.json.JSONModel"
          }
        }
    }
    
}